var c = document.getElementById("myCanvas");
var ctx = c.getContext("2d");
ctx.moveTo(80, 50);
ctx.lineTo(20, 300);
ctx.lineTo(100,450)
ctx.stroke();


var c = document.getElementById("myCanvas");
var ctx = c.getContext("2d");
ctx.beginPath();
ctx.arc(55,80,10,0,2*Math.PI);
ctx.fillStyle = "red";
ctx.fill();
ctx.stroke();

